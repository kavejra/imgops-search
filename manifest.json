{
    "manifest_version": 2,
    "name": "ImgOps Image Search",
    "description": "Upload image quickly to ImgOps, to imagesearch on Tineye, Google, Bing, Reddit, Yandex and other services.",
    "version": "1.0",
    "minumum_chrome_version": "22",
    "icons": {
        "16": "assets/images/search.png"
    },
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "persistent": false, 
        "scripts": [
            "assets/js/background.js"]
        }
}